<% if @entry.errors.any? %>
<ul>
  <% @entry.errors.full_messages.each do |error_msg| %>
  <li><%= @error_msg %></li>
  <% end %>
</ul>
<% end %>
<%= form_for [@project, @entry] do |f| %>
<%= f.label :hours %>
<%= f.text_field :hours, :autofocus => true, :class => 'test' %>
<br>
<%= f.label :minutes %>
<%= f.text_field :minutes %>
<br>
<%= f.label :date %>
<%= f.text_field :date %>
<br>
<%= f.submit 'Save' %>
<% end %>

